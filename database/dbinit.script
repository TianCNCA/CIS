CREATE TABLE CLIENTS ( ID INTEGER NOT NULL PRIMARY KEY, NAME VARCHAR( 30 ) NOT NULL UNIQUE, DOB VARCHAR( 30 ), HOMEPHONE VARCHAR( 23 ), WORKPHONE VARCHAR( 23 ), ADDRESS VARCHAR( 50 ), CITY VARCHAR( 30 ), PROVINCE VARCHAR( 4 ), POSTALCODE VARCHAR( 9 ), PHYSICHIAN BOOLEAN, PHYSTHER BOOLEAN, CHIRO BOOLEAN, PREVEXP BOOLEAN, REASON VARCHAR( 100 ), DIET BOOLEAN, MED BOOLEAN, INSULIN BOOLEAN, UNCTRL BOOLEAN, OCCUPATION VARCHAR( 35 ), SPORTS VARCHAR( 50 ), SLEEP VARCHAR( 20 ), SMOKING INTEGER, ALCOHOL INTEGER, STRESS INTEGER, APPETITE INTEGER );

CREATE TABLE SOAPS ( ID INTEGER NOT NULL PRIMARY KEY, NAME VARCHAR( 30 ) NOT NULL, DATE VARCHAR( 30 ) NOT NULL UNIQUE, DISC VARCHAR( 1024 ) NOT NULL );

CREATE TABLE ID ( KEY INTEGER NOT NULL PRIMARY KEY, ID INTEGER NOT NULL );

